<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.crayon.ve.mapper.WordCategoryMapper">
    <select id="listWordCategoryDTO" resultType="com.crayon.ve.POJO.DTO.WordCategoryDTO">
        SELECT id, `level`, description
        FROM word_category
        WHERE is_deleted = 0;
    </select>
</mapper>
